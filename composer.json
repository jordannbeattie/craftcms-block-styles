{
    "name": "jordanbeattie/craftcms-block-styles",
    "description": "Style dropdown for Neo blocks",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "jordan@jordanbeattie.co.uk"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0"
    },
    "autoload": {
        "psr-4": {
            "jordanbeattie\\blockstyles\\": "src/"
        }
    },
    "extra": {
        "handle": "block-styles",
        "name": "Block Styles",
        "documentationUrl": "",
        "class": "jordanbeattie\\blockstyles\\BlockStyles"
    },
    "authors": [
        {
            "name": "Jordan Beattie",
            "homepage": "https://jordanbeattie.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

